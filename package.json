{
  "name": "magic-transport",
  "version": "2.1.2",
  "description": "Transport for iframes",
  "main": "lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf lib",
    "build": "babel src --out-dir lib",
    "lint": "eslint '**/*.js'",
    "format": "prettier --write '**/*.js' && eslint '**/*.js' --fix",
    "size": "npm run build && size-limit",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "postpublish": "rm -rf lib && git push --follow-tags"
  },
  "author": "Sergey Pereskokov s.pereskokov@rambler-co.ru",
  "license": "ISC",
  "dependencies": {
    "dot-prop": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rambler-digital-solutions/magic-transport.git"
  },
  "bugs": {
    "url": "https://github.com/rambler-digital-solutions/magic-transport/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.17.1",
    "eslint-plugin-babel": "^4.1.1",
    "husky": "^0.14.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "lint-staged": "^7.0.4",
    "prettier": "^1.14.3",
    "size-limit": "^1.3.8",
    "webpack": "^2.1.0-beta.25"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
